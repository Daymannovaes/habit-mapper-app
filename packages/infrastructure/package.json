{
  "name": "@habit-mapper-app/infrastructure",
  "version": "0.1.1",
  "description": "> TODO: description",
  "author": "Dayman Novaes <daymannovaes@gmail.com>",
  "homepage": "https://github.com/Daymannovaes/habit-mapper-app#readme",
  "license": "ISC",
  "main": "dist/aws-exports.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daymannovaes/habit-mapper-app.git"
  },
  "scripts": {
    "postinstall": "yarn build",
    "amplify:pull": "amplify pull && yarn build",
    "amplify:push": "amplify push",
    "build": "echo '\n\tBUILDING INFRASTRUCTURE PACKAGE\n\n' && yarn build:database:config && yarn build:aws:config",
    "build:database:config": "bash ./tasks/build-database-config.sh",
    "build:aws:config": "bash ./tasks/build-aws-config.sh",
    "database:start:dev": "ENV=development bash ./database/scripts/start-db.sh",
    "database:migrate": "npx dotenv-cli -e database/configuration/.env.development -- db-migrate --config database/configuration/database.json --env dev --migrations-dir database/migrations",
    "database:migrate:create": "yarn database:migrate db:create habitmapper",
    "database:migrate:up": "yarn database:migrate up"
  },
  "bugs": {
    "url": "https://github.com/Daymannovaes/habit-mapper-app/issues"
  },
  "dependencies": {
    "@types/node": "^14.14.37",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "typescript": "^4.2.3"
  }
}
